.QuemSomos
  height: 100%
  margin-top: 135px;
  .content-holder
    height: 100%
    max-width: 1100px;
    margin:0 auto;
    padding: 5px 20px;
    box-sizing: border-box
    display:flex;
    flex-direction:row;
    h1
      color:#F14A08;
      font-weight:500;
    .textos
      width: 60%
      text-align:justify;
    .img-anuncie
      width:34%
    h2
      color:#F14A08;
      font-weight:300;
  .redes-sociais
    margin: 25px auto;
    display:flex
    justify-content: center;
    img
      margin: 0 6px
      cursor: pointer
  @media screen and (max-width: 800px)
    .content-holder
      .textos
        width: 95%
    .img-anuncie
      display: none;
    .imagem-mina
      display: none;
    .textos
      width:100%;
    .redes-sociais
      display: flex;
      justify-content: center;
      margin-top: 0.9em;
