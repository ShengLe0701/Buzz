.Anuncie
  height: 100%;
  margin-top: 115px;
  .content-holder
    height: 100%;
    max-width: 1100px;
    margin:0 auto;
    padding: 30px;
    padding-bottom: 0px;
    box-sizing: border-box
    display:flex;
    justify-content: space-around
    h1
      color:#F14A08;
      font-weight:500;
      margin-bottom: 30px
    .textos
      width: 60%
      text-align:justify;
      line-height: 1.4em
      .img-anuncie
        cursor: pointer;
    h2
      color:#F14A08;
      font-weight:300;
  .redes-sociais
    margin: 25px auto;
    display:flex
    justify-content: center;
    img
      margin: 0 6px
      cursor: pointer
  @media screen and (max-width: 800px)
    .content-holder
      flex-direction: column;
    .content-holder .textos
      width: 95%
      margin: 0 auto;
    .content-holder .img-holder
      display: none;
      .textos
        width:100%;
