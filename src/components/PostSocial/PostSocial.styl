#PostSocial
  width: 100%
  display: flex
  justify-content: space-between;
  padding: 25px 0px
  box-sizing: border-box
  .left
    display: flex
  .right
    display: flex

  .like-holder
    display: flex
    font-size: 1em
    font-weight: 400
    line-height: 3.2em
  .share-button
    display: flex
    background-color: red

  @media screen and (max-width: 800px)
    padding: 5px
